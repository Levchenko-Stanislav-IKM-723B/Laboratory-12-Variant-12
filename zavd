#include <iostream>
#include <vector>
using namespace std;

// Функція для отримання масиву дільників числа n
vector<int> getDivisors(int n) {
    vector<int> divisors;
    for (int i = 1; i <= n; ++i) {
        if (n % i == 0) {
            divisors.push_back(i);
        }
    }
    return divisors;
}

// Функція для перетворення масиву у зворотний порядок
vector<int> reverseArray(const vector<int>& arr) {
    vector<int> reversedArr;
    for (int i = arr.size() - 1; i >= 0; --i) {
        reversedArr.push_back(arr[i]);
    }
    return reversedArr;
}

int main() {
    // Введення числа n з клавіатури
    int n;
    cout << "Введіть ціле число n: ";
    cin >> n;

    // Отримання масиву дільників числа n
    vector<int> divisors = getDivisors(n);
    cout << "Дільники числа n: ";
    for (int divisor : divisors) {
        cout << divisor << " ";
    }
    cout << endl;

    // Перетворення масиву у зворотний порядок
    vector<int> reversedDivisors = reverseArray(divisors);
    cout << "Зворотний масив дільників: ";
    for (int divisor : reversedDivisors) {
        cout << divisor << " ";
    }
    cout << endl;

    return 0;
}
